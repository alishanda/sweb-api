{
  "name": "alexantella/sweb-api",
  "description": "Sweb API Client Implementation",
  "type": "library",
  "require": {
    "php": "^8.3",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4",
    "vlucas/phpdotenv": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "SwebApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwebApi\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check": [
      "@test"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}